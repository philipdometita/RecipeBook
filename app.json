{
  "expo": {
    "name": "Recipe Book",
    "slug": "RecipeBook",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#C1933E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#C1933E"
      },
      "package": "com.pdometita.RecipeBook",
      "blockedPermissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photoPermission": "The app accesses your photos to let you choose a photo for a recipe."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2cf51e35-4b6a-4fad-9091-bbcc74060b42"
      }
    }
  }
}
